{
  "manifest_version": 2,

  "name": "UMN Schedule Export",
  "description": "This extension selects class data from University of Minnesota schedules and formats them to be downloaded as ICS files to be used with iCal or Google Calendar",
  "version": "1.0",

  "icons": { "16": "icon16.png",
             "48": "icon48.png",
             "128": "icon128.png" },
 
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [ "https://cs.myu.umn.edu/*" ],
      "css": ["bootstrap.min.css"],
      "js": ["jquery-3.1.0.min.js", "bootstrap.min.js", "umn_cal.js"],
      "web_accessible_resources": ["jquery-3.1.0.min.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
